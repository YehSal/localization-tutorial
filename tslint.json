{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name" : [true, "never-prefix"],
    "no-unused-expression": false,
    "no-console": false
  },
  "jsRules": {
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-shadowed-variable": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}